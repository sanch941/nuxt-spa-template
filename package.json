{
    "name": "nuxt-spa-template",
    "version": "1.0.0",
    "description": "template for spa using vue by nuxt js",
    "author": "Sanzhar",
    "private": true,
    "scripts": {
        "dev": "nuxt",
        "build": "npm run img:resize && nuxt build",
        "start": "nuxt start",
        "generate": "nuxt generate --universal",
        "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
        "lint:prettier": "prettier --write \"**/*.*\"",
        "img:bg": "node scripts/img-bg-optimize.js",
        "img:normal": "node scripts/img-normal-optimize.js",
        "img:resize": "npm run img:bg && npm run img:normal"
    },
    "dependencies": {
        "axios": "^0.19.0",
        "nuxt": "^2.0.0",
        "object-fit-images": "^3.2.4",
        "tui-editor": "^1.4.10",
        "vue-notification": "^1.3.20"
    },
    "devDependencies": {
        "@fullhuman/postcss-purgecss": "^2.1.0",
        "@nuxtjs/eslint-config": "^1.0.1",
        "@nuxtjs/eslint-module": "^1.0.0",
        "@nuxtjs/tailwindcss": "^1.0.0",
        "async": "^3.1.0",
        "autoprefixer": "^9.7.4",
        "babel-eslint": "^10.0.1",
        "dotenv": "^8.2.0",
        "eslint": "^6.1.0",
        "eslint-config-prettier": "^4.1.0",
        "eslint-plugin-nuxt": ">=0.4.2",
        "eslint-plugin-prettier": "^3.0.1",
        "fs-extra": "^8.1.0",
        "glob": "^7.1.6",
        "imagemin": "^7.0.1",
        "imagemin-pngquant": "^8.0.0",
        "mkdirp-promise": "^5.0.1",
        "nuxt-purgecss": "^0.2.1",
        "postcss": "^7.0.27",
        "postcss-import": "^12.0.1",
        "postcss-preset-env": "^6.7.0",
        "prettier": "^1.16.4",
        "purgecss-webpack-plugin": "^2.1.0",
        "sharp": "^0.23.4",
        "tailwindcss": "^1.2.0"
    }
}
